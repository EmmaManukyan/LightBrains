<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="false" android:state_pressed="false">
        <shape android:shape="rectangle">
            <gradient android:angle="0" android:centerColor="#FFFFFF" android:endColor="#CFCECE" android:startColor="#CFCECE" />
        </shape>
    </item>

    <item android:state_focused="false" android:state_pressed="true">
        <shape android:shape="rectangle">
            <gradient android:angle="0" android:centerColor="#FFFFFF" android:endColor="#E1E1E1" android:startColor="#E1E1E1" />

        </shape>
    </item>
</selector>
