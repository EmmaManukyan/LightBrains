<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="false" android:state_pressed="false">
        <shape android:shape="rectangle">
            <corners android:radius="8dp" />
            <gradient android:angle="0" android:endColor="#FFA23A" android:startColor="#FFCABA" />
<!--
            <stroke android:width="1dp" android:color="@color/teal_700" />
-->
        </shape>
    </item>

    <item android:state_focused="false" android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="8dp" />
            <gradient android:angle="0" android:endColor="#FFCABA" android:startColor="#FFA23A" />
        </shape>
    </item>
</selector>